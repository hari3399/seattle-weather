# Example variables for terraform-azure-vm
vm_name = "example-vm"
vm_size = "Standard_B1ms"
location = "eastus"
# Name of the resource group (will be created if it doesn't exist)
resource_group_name = "rg-poc-dev"
# VNet and Subnet names (will be created if they don't exist in the resource group)
vnet_name = "shared-vnet"
subnet_name = "shared-subnet"
